
// ```
#include <iostream>
#include <vector>
#include <bits/stdc++.h>

using namespace std;


void setIO(string s) {
    ios_base::sync_with_stdio(0); cin.tie(0);
    freopen((s+".in").c_str(),"r",stdin);
    freopen((s+".out").c_str(),"w",stdout);
}
int main() {
    // freopen("shuffle.in", "r", stdin); freopen("shuffle.out", "w", stdout);

    ios::sync_with_stdio(false) ;
    cin.tie(nullptr) ;
    cout.tie(nullptr) ;


    //setIO("diamond");


    int n,ans=0 ;
    cin>>n;
    vector<int> v(n);
    for(int i=0;i<n;i++) {
        cin>>v[i];
    }
    for(int i=0;i<n;i++) {
        for(int j=0;j<n;j++) {
            float sum=0;
            int  a=0,z=j-i+1,t[z];
            for(int k=i;k<=j;k++) {
                t[a]=v[k];
                a++;
                sum+=v[k];
            }
            float av=(float)sum/(float)z;
            for(int k=0;k<z;k++) {
                if(av==t[k])
                {ans++;break;}
            }

        }
    }


    cout << ans ;


    return 0;
}

//```
